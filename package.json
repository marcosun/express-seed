{
  "name": "express-seed",
  "version": "0.0.1",
  "description": "Template for building express application.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "prebuild": "rimraf dist",
    "serve": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register dist/index.js",
    "start": "nodemon -e js,ts,tsx --exec ts-node -r tsconfig-paths/register src/index.ts"
  },
  "repository": "git@github.com:marcosun/express-seed.git",
  "author": "marcosun <marcoinbristol@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "connect-mongo": "^4.5.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^6.0.5",
    "regenerator-runtime": "^0.13.7",
    "tsconfig-paths": "^3.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/node": "^16.9.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.3"
  }
}
